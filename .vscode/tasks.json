{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "compile",
			"command": "/usr/bin/g++",
			"args": [
				"-O0",
				"-g3",
				"-Wall",
				"-Werror",
				"-std=c++11",
				"-fstrict-aliasing",
				"-Wstrict-aliasing",
				"-pthread",
				"-DNS3_BUILD_PROFILE_DEBUG",
				"-DNS3_ASSERT_ENABLE",
				"-DNS3_LOG_ENABLE",
				"-DHAVE_SYS_IOCTL_H=1",
				"-DHAVE_IF_NETS_H=1",
				"-DHAVE_NET_ETHERNET_H=1",
				"-DHAVE_PACKET_H=1",
				"-DHAVE_IF_TUN_H=1",
				"-I${fileDirname}/..",
				"-c",
				"${file}",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}.o"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"group": {
				"kind": "build",
				"isDefault": false
			},
			"detail": "compiler: /usr/bin/g++"
		},
		{
			"type": "cppbuild",
			"label": "build",
			"dependsOn": "compile",
			"command": "/usr/bin/g++",
			"args": [
				"-L${fileDirname}/../lib",
				"-Wl,-Bstatic",
				"-Wl,-Bdynamic",
				"-Wl,--no-as-needed",
				"-Llib",
				"-lns3.31-aodv-debug",
				"-lns3.31-test-debug",
				"-lns3.31-dsr-debug",
				"-lns3.31-mesh-debug",
				"-lns3.31-netanim-debug",
				"-lns3.31-wave-debug",
				"-lns3.31-wifi-debug",
				"-lns3.31-lr-wpan-debug",
				"-lns3.31-lte-debug",
				"-lns3.31-spectrum-debug",
				"-lns3.31-antenna-debug",
				"-lns3.31-applications-debug",
				"-lns3.31-csma-layout-debug",
				"-lns3.31-dsdv-debug",
				"-lns3.31-flow-monitor-debug",
				"-lns3.31-internet-apps-debug",
				"-lns3.31-wimax-debug",
				"-lns3.31-point-to-point-layout-debug",
				"-lns3.31-nix-vector-routing-debug",
				"-lns3.31-olsr-debug",
				"-lns3.31-sixlowpan-debug",
				"-lns3.31-tap-bridge-debug",
				"-lns3.31-internet-debug",
				"-lns3.31-bridge-debug",
				"-lns3.31-point-to-point-debug",
				"-lns3.31-traffic-control-debug",
				"-lns3.31-buildings-debug",
				"-lns3.31-propagation-debug",
				"-lns3.31-uan-debug",
				"-lns3.31-mobility-debug",
				"-lns3.31-energy-debug",
				"-lns3.31-config-store-debug",
				"-lns3.31-csma-debug",
				"-lns3.31-fd-net-device-debug",
				"-lns3.31-virtual-net-device-debug",
				"-lns3.31-topology-read-debug",
				"-lns3.31-network-debug",
				"-lns3.31-stats-debug",
				"-lns3.31-core-debug",
				"-lrt",
				"-pthread",
				"${fileBasenameNoExtension}.o",
				"-o",
				"${fileBasenameNoExtension}"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: /usr/bin/g++"
		}
	]
}