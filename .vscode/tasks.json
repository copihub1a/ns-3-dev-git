{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "compile",
			"command": "/usr/bin/g++",
			"args": [
				"-O0",
				"-g3",
				"-Wall",
				"-Werror",
				"-std=c++11",
				"-fstrict-aliasing",
				"-Wstrict-aliasing",
				"-pthread",
				"-DNS3_BUILD_PROFILE_DEBUG",
				"-DNS3_ASSERT_ENABLE",
				"-DNS3_LOG_ENABLE",
				"-DHAVE_SYS_IOCTL_H=1",
				"-DHAVE_IF_NETS_H=1",
				"-DHAVE_NET_ETHERNET_H=1",
				"-DHAVE_PACKET_H=1",
				"-DHAVE_IF_TUN_H=1",
				"-I${fileDirname}/..",
				"-c",
				"${file}",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}.o"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"group": {
				"kind": "build",
				"isDefault": false
			},
			"detail": "compiler: /usr/bin/g++"
		},
		{
			"type": "cppbuild",
			"label": "build",
			"dependsOn": "compile",
			"command": "/usr/bin/g++",
			"args": [
				"-Wl,--no-as-needed",
				"-L${fileDirname}/../lib",
				"-lns3.31-applications-debug",
				"-lns3.31-internet-debug",
				"-lns3.31-bridge-debug",
				"-lns3.31-mpi-debug",
				"-lns3.31-traffic-control-debug",
				"-lns3.31-csma-debug",
				"-lns3.31-network-debug",
				"-lns3.31-stats-debug",
				"-lns3.31-core-debug",
				"-lrt",
				"-pthread",
				"${fileBasenameNoExtension}.o",
				"-o",
				"${fileBasenameNoExtension}"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: /usr/bin/g++"
		}
	]
}